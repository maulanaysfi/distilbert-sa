apiVersion: v1
kind: Service
metadata:
  name: jupyterlab-nfs
  labels:
    app.kubernetes.io/name: jupyterlab
    app.kubernetes.io/instance: jupyterlab
    app.kubernetes.io/version: "1.16.0"
spec:
  type: ClusterIP
  ports:
    - port: 21
      protocol: TCP
      targetPort: 21
      name: ftp-control
    - port: 20
      protocol: TCP
      targetPort: 20
      name: ftp-data
  selector:
    app.kubernetes.io/name: jupyterlab
    app.kubernetes.io/instance: jupyterlab


